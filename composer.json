{
    "name": "fuelviews/laravel-init",
    "description": "Laravel init package",
    "keywords": [
        "fuelviews",
        "laravel",
        "init"
    ],
    "homepage": "https://github.com/fuelviews/laravel-init",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Mitchener",
            "homepage": "https://fuelviews.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "fuelviews/laravel-cloudflare-cache": "^1.0",
        "fuelviews/laravel-robots-txt": "^0.0",
        "fuelviews/laravel-sabhero-wrapper": "^0.0",
        "fuelviews/laravel-sitemap": "^0.0",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "livewire/livewire": "^3.6",
        "ralphjsmit/laravel-glide": "^1.3",
        "ralphjsmit/laravel-seo": "^1.7",
        "spatie/laravel-google-fonts": "^1.4",
        "spatie/laravel-package-tools": "^1.16.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^10.2||^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0||^2.34",
        "pestphp/pest-plugin-arch": "^3.0||^2.7",
        "pestphp/pest-plugin-laravel": "^3.2||^2.3",
        "rector/rector": "^2.0",
        "spatie/image-optimizer": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Fuelviews\\Init\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fuelviews\\Init\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-init --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fuelviews\\Init\\InitServiceProvider"
            ]
        },
        "aliases": {
            "Init": "Fuelviews\\Init\\Facades\\Init"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
